{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/preload/*.d.ts",
    "src/types/*",
    "src/utils/*",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*",
      ],
      "@src/*":["./src/*"]
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": ["dom", "es6"]

  }
}
