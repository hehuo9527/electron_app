{
  "name": "rcssetup",
  "version": "1.0.0",
  "description": "VPLite App",
  "main": "./out/main/index.js",
  "homepage": "index.html",
  "author": "sss.com",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": " electron-vite dev -w",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "electron:generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "reinstall": "electron-rebuild -f -w ffi-napi ref-napi"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@element-plus/icons": "^0.0.11",
    "@element-plus/icons-vue": "^2.3.1",
    "electron-icon-builder": "^2.0.1",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.7.2",
    "mitt": "^3.0.1",
    "mqtt": "^5.8.1",
    "phantomjs": "^2.1.7",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/axios": "^0.14.0",
    "@types/node": "^18.19.32",
    "@types/qs": "^6.9.15",
    "@types/vue-i18n": "^7.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vueuse/electron": "^10.9.0",
    "axios": "^1.6.8",
    "babel-eslint": "^10.1.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^5.1.4",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "element-plus": "^2.7.4",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "iconv-lite": "^0.6.3",
    "jwt-decode": "^4.0.0",
    "node-schedule": "^2.1.1",
    "obs-websocket-js": "^5.0.5",
    "prettier": "^3.2.4",
    "qs": "^6.12.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.5",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.15",
    "vue-i18n": "^9.13.1",
    "vue-pick-colors": "^1.6.5",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.29",
    "ws": "^8.18.0"
  }
}
